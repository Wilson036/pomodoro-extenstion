{
    "name": "Pomodoro Focus",
    "description": "AI-powered Pomodoro timer & site blocker",
    "version": "0.1.0",
    "manifest_version": 3,
  
    "action": {
      "default_popup": "src/popup/index.html",
      "default_icon": "icons/icon.png"
    },

    "options_page": "src/options/index.html",
    "background": {
      "service_worker": "src/background.js"
    },
    "permissions": [
      "storage"
    ]



  }